{
  "expo": {
    "name": "Steps Tracker",
    "slug": "steps-tracker-app",
    "version": "1.2.4",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4285F4"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.stepstracker.app"
    },
    "android": {
      "package": "com.stepstracker.app",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c2ff325e-afb4-448a-8fa6-189bebce4384"
      }
    }
  }
}
